{
  "name": "bun-monorepo",
  "version": "0.0.1",
  "description": "Elysia and NextJS monorepo starter",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"cd apps/backend && bun dev\"  \"cd apps/web && bun run dev\"",
    "typecheck": "bunx --bun tsc"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  },
  "dependencies": {
    "@react-aria/visually-hidden": "^3.8.14"
  }
}
